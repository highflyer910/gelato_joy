{
    "builds": [
      {
        "src": "FE/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "BE/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "BE/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "FE/.next/$1"
      }
    ],
    "buildCommand": "npm --prefix FE run build",
  "outputDirectory": "FE/.next",
  "framework": "nextjs"
  }
  